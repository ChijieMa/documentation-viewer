{
  "name": "documentation-viewer",
  "version": "0.1.0",
  "description": "View documentation generated by documentation.js",
  "main": "index.js",
  "scripts": {
    "dev": "WEBPACK_ENV=dev webpack-dev-server",
    "start": "firebase serve -p ${PORT:-8080}",
    "prestart": "npm run build",
    "build": "mkdir -p build && WEBPACK_ENV=production webpack && cp -r src/{index.html,manifest.json} build/",
    "deploy": "firebase deploy"
  },
  "keywords": [
    "preact",
    "documentation"
  ],
  "author": "Jason Miller <jason@developit.ca>",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-loader": "^5.3.3",
    "css-loader": "^0.21.0",
    "exports-loader": "^0.6.2",
    "extract-text-webpack-plugin": "^0.8.2",
    "firebase-tools": "^2.0.2",
    "http-server": "^0.8.5",
    "less-loader": "^2.2.0",
    "ncp": "^2.0.0",
    "normalize.css": "^3.0.3",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.1",
    "webpack-dev-server": "^1.10.1"
  },
  "dependencies": {
    "babel-runtime": "^5.8.20",
    "decko": "^1.1.0",
    "material-design-lite": "^1.0.5",
    "object-assign": "^4.0.1",
    "preact": "^1.5.1"
  }
}
